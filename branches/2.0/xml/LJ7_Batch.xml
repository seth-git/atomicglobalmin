<?xml version="1.0"?>
<agml version="2.0"
      xmlns="http://sourceforge.net/projects/atomicglobalmin/"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation= "http://sourceforge.net/projects/atomicglobalmin/ agml.xsd">
	<batch>
		<setup>
			<structuresTemplate>
				<structureTemplate>
					<atomTemplate number="7" Z="He"/>
				</structureTemplate>
				<threeD number="100" constraints="const"/>
			</structuresTemplate>
		</setup>
		<constraints name="const">
			<cube size="5"/>
			<atomicDistances>
				<min value="1.0"/>
				<max value="1.5"/>
			</atomicDistances>
		</constraints>
		<energy>
			<internal method="Lennard Jones"/>
		</energy>
	</batch>
</agml>
