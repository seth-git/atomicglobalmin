<?xml version="1.0"?>
<agml version="2.0"
      xmlns="http://sourceforge.net/projects/atomicglobalmin/"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation= "http://sourceforge.net/projects/atomicglobalmin/ agml.xsd">
	<simulatedAnnealing constraints="const">
		<setup saveFrequency="10">
			<populationTemplate constraints="const">
				<structureTemplate>
					<atomTemplate number="7" z="2" />
				</structureTemplate>
				<threeD number="32"/>
				<seed />
			</populationTemplate>
			<temperature acceptedPerterbations="90" decreaseAfterIteration="200"/>
			<annealingSchedule quenchingFactor="0.9995"/>
			<perturbations>
				<translationVector startLength="0.3" minLength="0.01" probability="1" />
			</perturbations>
			<stop minIterations="20000" maxTemperature="400" maxAcceptedPerturbations="10" />
		</setup>
		<constraints name="const">
			<cube size="5.5"/>
			<atomicDistances>
				<min value="1.0" />
				<max value="1.5" />
			</atomicDistances>
		</constraints>
		<energy>
			<internal method="Lennard Jones"/>
		</energy>
	</simulatedAnnealing>
</agml>
